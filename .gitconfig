[user]
	name = <my_name>
	email = <my_email_address>
[push]
	default = simple
[alias]
	s = status
	d = diff
	g = log --pretty=oneline --abbrev-commit
	a = add
	co = checkout
	ci = commit
	cm = commit -m
	ca = commit -a
	cam = commit -am
	p = push
	graph = log --graph --oneline --all
[credential]
	helper = store
